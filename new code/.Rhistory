data_list[[1]] <- mother_wavelet
# Add the functions Delta_n(t) for n = 1 to n_max
for (n in 1:n_max) {
j <- floor(log2(n))
k <- n - 2^j
wavelet_vals <- sapply(t_vals, Delta_n, j = j, k = k)
