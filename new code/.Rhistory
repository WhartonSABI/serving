# Define the "mother wavelet" H(t)
H <- function(t) {
if (t >= 0 && t < 0.5) {
return(1)
} else if (t >= 0.5 && t <= 1) {
return(-1)
} else {
return(0)
}
}
# Define the sequence of functions H_n(t)
H_n <- function(t, j, k) {
return(2^(j/2) * H((2^j) * t - k))
}
